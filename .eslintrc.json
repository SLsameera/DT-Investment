{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "no-console": "off",
    "eqeqeq": ["error", "smart"],
    "curly": "warn",
    "no-useless-escape": "off",
    "no-prototype-builtins": "warn",
    "comma-dangle": ["error", "never"]
  }
}
